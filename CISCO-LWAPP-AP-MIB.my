cLApGlobalAPDtlsCipherSuite OBJECT-TYPE
      SYNTAX INTEGER {
          rsaaes128sha1(47),
          rsaaes256sha1(53),
          rsaaes256sha256(61),
          rsagcm128sha256(156),
          rsagcm256sha384(157),
          ecdsaaes128gcmsha256(49195),
          ecdsaaes256gcmsha384(49196)
      }
      MAX-ACCESS  read-write
      STATUS      current
      DESCRIPTION
          "This object specifies the dtls cipher suite of ap"
