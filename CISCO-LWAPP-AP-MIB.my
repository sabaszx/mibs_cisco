-- ********************************************************************
-- *    AP BLE Antenna information
-- ********************************************************************

cLApBleAntennaTable OBJECT-TYPE
    SYNTAX         SEQUENCE OF CLApBleAntennaEntry
    MAX-ACCESS     not-accessible
    STATUS         current
    DESCRIPTION
       "This table represents information about BLE Antenna
       present on Access Points"
    ::= { ciscoLwappApBleManagement 1 }

cLApBleAntennaEntry OBJECT-TYPE
    SYNTAX         CLApBleAntennaEntry
    MAX-ACCESS     not-accessible
    STATUS         current
    DESCRIPTION
        "Entry contains BLE Antenna Information of particular radio slot of the
        Access Point."
    INDEX {
               cLApSysMacAddress,
               cLApDot11IfSlotId,
               cLApDot11IfAntennaId 
          }
    ::= { cLApBleAntennaTable 1 }

CLApBleAntennaEntry::=SEQUENCE {
        cLApBleAntennaPresent              TruthValue, 
   	cLApBleAntennaPid              	   SnmpAdminString,
        cLApBleAntennaGain	           INTEGER,
        cLApBleAntennaType     		   INTEGER,
        cLApBleAntennaMode    	           INTEGER,
        cLApBleAntennaDiversityStatus      INTEGER, 
	cLApBleAntennaOptions              INTEGER 
    }

cLApBleAntennaPresent OBJECT-TYPE
    SYNTAX         TruthValue 
    MAX-ACCESS     read-only
    STATUS         current
    DESCRIPTION
        "This object represents if the Wifi slot is overloaded or not.
        True - If the slot has overloaded BLE antenna 
        False - If the slot has only WiFi Antenna i.e,no overloaded antenna."
    DEFVAL         { false }
    ::= { cLApBleAntennaEntry 1 }

cLApBleAntennaPid OBJECT-TYPE
    SYNTAX         SnmpAdminString
    MAX-ACCESS     read-only
    STATUS         current
    DESCRIPTION
       "This object represents the antenna product ID for BLE module."
    ::= { cLApBleAntennaEntry 2 }

cLApBleAntennaGain OBJECT-TYPE
    SYNTAX         INTEGER(0..40)
    UNITS          "dBi"
    MAX-ACCESS     read-only
    STATUS         current
    DESCRIPTION
        "This object represents the gain for BLE antenna."
    ::= { cLApBleAntennaEntry 3 }

cLApBleAntennaType OBJECT-TYPE
    SYNTAX         INTEGER {
                     notApplicable(1),
                     integrated(2),
                     usbModule(3)
                   }
    MAX-ACCESS     read-only
    STATUS         current
    DESCRIPTION
        "This object represents if the BLE antenna currently used by the 
        access point is integrated or USB module.
        1 - No BLE Antenna present
        2 - Integrated BLE antenna(internal)
        3 - BLE antenna in USB module(external)"
    ::= { cLApBleAntennaEntry 4 }
 
cLApBleAntennaMode OBJECT-TYPE
    SYNTAX         INTEGER {
                     notApplicable(1),
                     omni(2)
                   }
    MAX-ACCESS     read-only
    STATUS         current
    DESCRIPTION
        "This object represents the mode in which the BLE antenna operates."
    ::= { cLApBleAntennaEntry 5 }

cLApBleAntennaDiversityStatus OBJECT-TYPE
    SYNTAX         INTEGER {
                     notApplicable(1),
                     enabled(2)
                   }
    MAX-ACCESS     read-only
    STATUS         current
    DESCRIPTION
        "This object represents the diversity status for BLE antenna."
    ::= { cLApBleAntennaEntry 6 }

cLApBleAntennaOptions OBJECT-TYPE
    SYNTAX         INTEGER {
                     notApplicable(1),
                     internal(2),
                     external(3)
                   }
    MAX-ACCESS     read-only
    STATUS         current
    DESCRIPTION
        "This object represents the BLE Antenna type/option available on 
        the access point whether it supports internal or external BLE radio.
        1 - No BLE Antenna present
        2 - Internal BLE antenna(Integrated)
        3 - External BLE antenna(USB module)"
    ::= { cLApBleAntennaEntry 7 }

