cLApPrimedDiscoveryTimeout OBJECT-TYPE
		SYNTAX	INTEGER (0 | 120..43200)
        UNITS       "seconds"
		MAX-ACCESS	read-write
		STATUS		current
		DESCRIPTION
          "This object specifies the primed timeout for the AP
           discovery."
		::= { ciscoLwappApGlobal 23 }

cLApGlobalPreferMode OBJECT-TYPE
      SYNTAX INTEGER {
          ipv4(1),
          ipv6(2)
      }
